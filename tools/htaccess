DirectoryIndex index.php index.html
<IfModule mod_rewrite.c>
RewriteEngine On
RewriteBase /

# 実体があるかもしれないファイルは条件判定
RewriteCond %{DOCUMENT_ROOT}/%{REQUEST_URI} !-f
RewriteCond %{DOCUMENT_ROOT}/%{REQUEST_URI} !-d
# フレームワークアプリを起動
RewriteRule ^(.*)$ biscuit/$1

</IfModule>

AddType video/mp4 .mp4

